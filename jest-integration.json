{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "./",
  "testEnvironment": "node",
  "testRegex": ".test.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "testTimeout": 900000,
  "setupFiles": ["<rootDir>/tests/utils/integration/setEnvVars.ts"],
  "moduleDirectories": ["node_modules", "src", "<rootDir>"]
}
